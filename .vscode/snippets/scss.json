{
	"abstracts snippet": {
		"scope": "scss",
		"prefix": "abs",
		"body": ["@use 'abstracts/$1' as *;"],
		"description": "abstracts snippet"
	},

	"media query": {
		"prefix": "mq",
		"body": ["@include breakpoint($1) {$2}"],
		"description": "media query"
	},

	"media query down": {
		"prefix": "mqd",
		"body": ["@include breakpoint-down($1) {$2}"],
		"description": "media query"
	}
}
